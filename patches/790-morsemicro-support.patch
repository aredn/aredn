+	vmmc-supply = <&mmc_reg_3v3>;
+	vqmmc-supply = <&mmc_reg_3v3>;
+	cap-sdio-irq;
+	non-removable;
+	disable-wp;
+
+	mm6108_sdio: mm6108_sdio@0 {
+			compatible = "morse,mm610x";
+			reset-gpios = <&gpio 40 0>;
+			power-gpios = <&gpio 41 0>,
+			              <&gpio 39 0>;
+			status = "okay";
+			reg = <2>;
+			bus-width = <4>;
+	};
+};
