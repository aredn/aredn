+	spis_pins: spis{
+		spis{
+			groups = "spis";
+			function = "pwm_uart2";
+		};
+	};
+};
+
+&state_default {
+	gpio {
+		groups = "wled_an", "refclk", "wdt", "p2led_an", "p3led_an", "p4led_an";
+		function = "gpio";
+	};
+};
+
+&wmac {
+	status = "okay";
+
+	mediatek,mtd-eeprom = <&factory 0x0>;
+};
+
+&ethernet {
