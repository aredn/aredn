{% if (!auth.isAdmin) { %}
<script>
(function(){
const l = document.getElementById("{{inner}}");
function x(e) {
    l.removeEventListener("click", x);
    window.injectCallback = () => { htmx.process(l); htmx.trigger(e.target, "click") }
    fetch("/a/inject").then(v => v.text()).then(v => eval(v));
}
l.addEventListener("click", x);
})();
</script>
