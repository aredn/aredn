<div id="nav-status" {{request.headers["hx-boosted"] ? 'hx-swap-oob="true"' : ""}}>{%
if (request.page === "status") {
    if (auth.isAdmin) {
        print("admin");
    }
    else {
        print("status");
    }
}
else if (request.page === "mesh") {
    const issupernode = uci.get("aredn", "@supernode[0]", "enable") == "1";
    if (issupernode) {
        print("cloud");
    }
    else {
        print("mesh");
    }
}
else {
    print(request.page);
}
%}</div>
