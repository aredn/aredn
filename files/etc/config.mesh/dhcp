config dhcp
	option interface	lan
	option start 		<dhcp_start>
	option limit		<dhcp_limit>
	option leasetime	1h
        option ignore           <lan_dhcp>


config dhcp
	option interface	wan
	option ignore		1

config dhcp
	option interface	wifi
	option ignore		1

config dnsmasq
	option resolvfile 	'/tmp/resolv.conf.d/resolv.conf.auto'
