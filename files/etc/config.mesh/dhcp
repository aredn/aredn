config dnsmasq
	option rebind_protection '0'
	option confdir		'/tmp/dnsmasq.d,*.conf'

config dhcp
	option interface	lan
	option start 		<dhcp_start>
	option limit		<dhcp_limit>
	option leasetime	1h
    option ignore		<lan_dhcp>
	option force		1

config dhcp
	option interface	wan
	option ignore		1

config dhcp
	option interface	wifi
	option ignore		1
